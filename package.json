{
  "name": "fxhash-boilerplate",
  "version": "2.0.0",
  "description": "The fxhash boilerplate, comes with some utilities to create a project for fxhash",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "postinstall": "node ./lib/scripts/install.js",
    "start": "node ./lib/scripts/start.js",
    "start:project": "webpack serve --config ./lib/config/webpack.config.dev.js",
    "build": "webpack --config ./lib/config/webpack.config.prod.js"
  },
  "author": "fxhash",
  "license": "MIT",
  "dependencies": {
    "@dimforge/rapier2d": "^0.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@webvoxel/fast-simplex-noise": "^0.0.1-a2",
    "adm-zip": "^0.5.9",
    "babel-loader": "^9.1.2",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "github-download-directory": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jsoncrush": "^1.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "three": "^0.153.0",
    "ts-loader": "^9.4.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "typescript": "^5.1.6"
  }
}
